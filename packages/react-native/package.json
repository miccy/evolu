{
  "name": "@evolu/react-native",
  "version": "12.0.1-preview.4",
  "description": "Evolu for React Native and Expo",
  "keywords": [
    "evolu",
    "expo",
    "expo-sqlite",
    "op-sqlite",
    "react-native",
    "react-hooks"
  ],
  "author": "Daniel Steigerwald <daniel@steigerwald.cz>",
  "license": "MIT",
  "repository": "evoluhq/evolu",
  "bugs": {
    "url": "https://github.com/evoluhq/evolu/issues"
  },
  "homepage": "https://evolu.dev",
  "type": "module",
  "exports": {
    "./expo-sqlite": {
      "types": "./dist/expo-sqlite.d.ts",
      "default": "./dist/expo-sqlite.js",
      "react-native": "./dist/expo-sqlite.js",
      "browser": "./dist/web.js"
    },
    "./op-sqlite": {
      "types": "./dist/op-sqlite.d.ts",
      "default": "./dist/op-sqlite.js",
      "react-native": "./dist/op-sqlite.js",
      "browser": "./dist/web.js"
    }
  },
  "typesVersions": {
    "*": {
      "expo-sqlite": [
        "./dist/expo-sqlite.d.ts"
      ],
      "op-sqlite": [
        "./dist/op-sqlite.d.ts"
      ]
    }
  },
  "files": [
    "dist/**",
    "src/**",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "shx rm -rf dist && tsc",
    "clean": "shx rm -rf .turbo node_modules dist",
    "format": "prettier --write \"src/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@evolu/common": "workspace:*",
    "@evolu/react": "workspace:*",
    "@evolu/tsconfig": "workspace:*",
    "@op-engineering/op-sqlite": "^14.1.4",
    "expo": "^53.0.20",
    "expo-sqlite": "~15.2.14",
    "react-native": "^0.79.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.3"
  },
  "peerDependencies": {
    "@evolu/common": "^6.0.1-preview.19",
    "@evolu/react": "^9.0.1-preview.4",
    "@op-engineering/op-sqlite": ">=12",
    "expo": ">=52",
    "expo-sqlite": ">=15",
    "react-native": ">=0.76"
  },
  "peerDependenciesMeta": {
    "@op-engineering/op-sqlite": {
      "optional": true
    },
    "expo": {
      "optional": true
    },
    "expo-sqlite": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "sideEffects": []
}
